					xmlns:viewModels ="clr-namespace:Jeon.CommonSampleWindow.ViewModels"
					xmlns:views="clr-namespace:Jeon.CommonSampleWindow.Views">

	<DataTemplate DataType="{x:Type viewModels:MainViewModel}">
		<views:MainView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type viewModels:EventPopupViewModel}">
		<views:EventPopupView />
	</DataTemplate>

</ResourceDictionary>